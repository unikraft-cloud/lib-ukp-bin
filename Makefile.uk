ifeq ($(CONFIG_LIBMUSL),y)
LIBUKP_OLIBC:=-musl
else
LIBUKP_OLIBC:=-nolibc
endif
ifeq ($(CONFIG_LIBPOSIX_SOCKET_EVENTS),y)
LIBUKP_OSOCK:=-sock
else
LIBUKP_OSOCK:=
endif
ifeq ($(CONFIG_OPTIMIZE_PIE),y)
LIBUKP_OPIE:=-pie
else
LIBUKP_OPIE:=-pde
endif

UK_OLIBS-$(CONFIG_LIBUKP) += $(IMPORT_BASE)/libukp-$(CONFIG_UK_ARCH)$(LIBUKP_OLIBC)$(LIBUKP_OSOCK)$(LIBUKP_OPIE).o
UK_LIBID_EXTRA-$(CONFIG_LIBUKP) += libukp
