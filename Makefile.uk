ifeq ($(CONFIG_LIBMUSL),y)
LIBUKP_OLIBC:=-musl
else
LIBUKP_OLIBC:=-nolibc
endif
ifeq ($(findstring $(CONFIG_LIBVFSCORE),$(CONFIG_LIBUKNETDEV)),y)
LIBUKP_OVFS:=-vfs
LIBUKP_ONET:=-net
else
LIBUKP_OVFS:=
LIBUKP_ONET:=
endif
ifeq ($(CONFIG_OPTIMIZE_PIE),y)
LIBUKP_OPIE:=-pie
else
LIBUKP_OPIE:=
endif

UK_OLIBS-$(CONFIG_LIBUKP) += $(IMPORT_BASE)/libukp-$(CONFIG_UK_ARCH)$(LIBUKP_OVFS)$(LIBUKP_ONET)$(LIBUKP_OLIBC)$(LIBUKP_OPIE).o
UK_LIBID_EXTRA-$(CONFIG_LIBUKP) += libukp
